{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "proxy": "node proxy-server/index.js",
    "dev": "concurrently \"npm run start\" \"npm run proxy\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:api": "openapi --input ../api/private/backend-api.yaml --output ./src/api-generated/backend && openapi --input ../api/public/coinbase-public-api.yaml --output ./src/api-generated/coinbase && openapi --input ../api/public/coinbase-exchange-public-api.yaml --output ./src/api-generated/coinbase-exchange",
    "generate:backend-api": "openapi --input ../api/private/backend-api.yaml --output ./src/api-generated/backend",
    "generate:coinbase-api": "openapi --input ../api/public/coinbase-public-api.yaml --output ./src/api-generated/coinbase",
    "generate:coinbase-exchange-api": "openapi --input ../api/public/coinbase-exchange-public-api.yaml --output ./src/api-generated/coinbase-exchange"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-data-grid": "^8.1.0",
    "@tanstack/react-query": "^5.74.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.0.0",
    "i18next-browser-languagedetector": "^8.0.4",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
